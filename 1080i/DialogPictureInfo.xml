						<height>60</height>
						<font>font15</font>
						<align>right</align>
						<label>$INFO[ListItem.Label2]</label>
						<textoffsetx>15</textoffsetx>
					</control>
				</focusedlayout>
			</control>
			<control type="scrollbar" id="60">
				<left>1092</left>
				<top>88</top>
				<height>600</height>
