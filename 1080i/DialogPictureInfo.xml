					</control>
				</itemlayout>
				<focusedlayout height="60" width="1058">
					<control type="image">
						<width>1058</width>
						<height>60</height>
						<texture border="4">dialogs/separator2.png</texture>
					</control>
					<control type="image">
						<width>1057</width>
						<height>58</height>
						<texture border="4" colordiffuse="$VAR[SkinThemeColour]">dialogs/listselect_fo.png</texture>
