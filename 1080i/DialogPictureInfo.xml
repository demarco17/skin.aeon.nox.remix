<?xml version="1.0" encoding="UTF-8"?>
<window>
	<defaultcontrol always="true">5</defaultcontrol>
	<include>OpenCloseAnimation</include>
	<controls>
		<control type="group">
			<left>399</left>
			<top>180</top>
			<control type="image">
