			<top>-16</top>
			<width>494</width>
			<height>1116</height>
			<texture border="18">dialogs/dialog-bg2.png</texture>
		</control>
		<control type="grouplist" id="996">
			<description>grouplist for context buttons</description>
			<left>32</left>
			<width>462</width>
			<height max="1080">auto</height>
		</control>
